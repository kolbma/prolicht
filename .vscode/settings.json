{
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "java.test.config": [
        {
            "name": "prolicht",
            "env": {
                "prolichtuser": "user",
                "prolichtpassword": "123456"
            },
            "workingDirectory": "${workspaceFolder}"
        }
    ],
    "java.test.defaultConfig": "prolicht",
    "maven.terminal.customEnv": [
        {
            "environmentVariable": "PROLICHTUSER",
            "value": "user"
        },
        {
            "environmentVariable": "PROLICHTPASSWORD",
            "value": "123456"
        }
    ]
}
